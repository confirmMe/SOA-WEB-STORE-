package sw;
public class te{
	
	public static int fd(int[] arr){
		
		 Integer max = Integer.MIN_VALUE;//无穷小
		   int cur = 0;
		       
		   for (int i = 0; i < arr.length; i++) {
		      cur += arr[i];

		      max = Math.max(max, cur);

		      cur = cur < 0 ? 0 : cur;
		   }
		   return max;
		
	}
	
	
	
}