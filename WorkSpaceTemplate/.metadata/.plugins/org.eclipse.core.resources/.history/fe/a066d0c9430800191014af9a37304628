#首页大广告位信息的redis的hash主键
INDEX_CONTENT_REDIS_KEY=adverKey
#左侧商品分类的redis的hash主键
INDEX_CAT_REDIS_KEY=catKey
#左侧商品分类的redis的默认第二主键
