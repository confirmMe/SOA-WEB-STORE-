package Test01;

public    class   te2   {
	class Person{
		private int  age;

		public Person(int age){
			this.age=age;
		}
		public int getAge() {
			return age;
		}

		public void setAge(int age) {
			this.age = age;
		}
		
	}
	public static void setT(Person person){
		 person.age+=10;
		
	}
	public static void main(String[] args) {
		Person person=new Person(10);
		setT(person);
		System.out.println(person.age);
	}
}
