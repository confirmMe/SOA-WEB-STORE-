package Test01;


public class Tesrt {
 
		 public static int fd(int [] arr){
			 int max=0;
			 int sum=0;
			 for(int i=0;i<arr.length;i++)
			 sum=sum+arr[i];
			 max=Integer.max(max, sum);
			 sum=sum>0?sum:0;
			 return max;
			 
		 }
		 public static void main(String[] args) {
			int [] arr={1,2,5,-16,14,2};
			System.out.println(fd(arr));
		}

}
